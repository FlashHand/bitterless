{
  "name": "bitterless",
  "version": "1.0.0",
  "versionCode": 2026021501,
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "repository": "git@github.com:FlashHand/bitterless.git",
  "author": "WangBo <ralwayne@163.com>",
  "license": "MIT",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "rig --env debug_dev && electron-vite dev",
    "build": "yarn typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps && rig postinstall",
    "build:unpack": "yarn build && electron-builder --dir",
    "build:win": "yarn build && electron-builder --win",
    "build:mac": "yarn build && electron-builder --mac",
    "build:linux": "yarn build && electron-builder --linux",
    "preinstall": "rig preinstall"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@langchain/core": "^1.1.24",
    "@langchain/langgraph": "^1.1.4",
    "@langchain/openai": "^1.2.7",
    "@qdrant/js-client-rest": "^1.16.2",
    "axios": "^1.13.5",
    "better-sqlite3-multiple-ciphers": "^12.6.2",
    "commander": "^14.0.3",
    "dayjs": "^1.11.19",
    "eventsource": "^4.1.0",
    "markstream-vue": "^0.0.7-beta.8",
    "moment": "^2.30.1",
    "node-llama-cpp": "^3.15.1",
    "postman-request": "^2.88.1-postman.48",
    "quill": "^2.0.3",
    "rig-foundation": "git+ssh://git@github.com:FlashHand/rig-foundation.git#1.0.1",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.2",
    "zod": "^4.3.6",
    "electron-buff": "git+ssh://git@github.com:FlashHand/electron-buff.git#1.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/node": "^22.19.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "electron": "^40.4.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "json5": "2.2.1",
    "less": "^4.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "rig-helper": "^1.0.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vue": "^3.5.25",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.6"
  },
  "private": true,
  "workspaces": [
    "rigs/*",
    "rig_dev/*"
  ]
}